{
  "name": "@useprint/shared",
  "version": "0.1.1",
  "description": "Shared constants and utilities for useprint packages",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup-node",
    "build:watch": "tsup-node --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxscn/useprint.git",
    "directory": "packages/shared"
  },
  "keywords": [
    "react",
    "document"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "8.4.0",
    "typescript": "5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
